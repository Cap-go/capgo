        {
          "binding": "HYPERDRIVE_DB_SG",
          "id": "06ee738d905d43d1a055682face2dddb"
        },
        {
          "binding": "HYPERDRIVE_DB_US",
          "id": "51df4318bd5d4a35a5c4c145b520b22f"
        }
      ],
      // Do not place smart as we want to use read replicas in specific regions
      "placement": {
        "mode": "off"
      }
    },
    "preprod": {
      "name": "capgo_plugin-preprod",
      "vars": {
        "IS_V2": "0",
        "ENV_NAME": "capgo_plugin-preprod"
