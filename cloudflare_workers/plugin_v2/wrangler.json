{
  "name": "capgo_plugin-v2-prod",
  "vars": {
    "is_v2": "1"
  },
  "routes": [
    {
      "pattern": "plugin_v2.capgo.app",
      "custom_domain": true
    },
    {
      "pattern": "plugin_v2.usecapgo.com",
      "custom_domain": true
    }
  ],
  "logpush": true,
  "compatibility_date": "2025-09-15",
  "main": "./index.ts",
  "compatibility_flags": [
    "nodejs_compat_v2",
    "nodejs_compat_populate_process_env"
  ],
  "workers_dev": false,
  "observability": {
    "enabled": true,
    "head_sampling_rate": 0.001
  },
  "placement": {
    "mode": "smart"
  },
  "upload_source_maps": true,
  "analytics_engine_datasets": [
    {
      "binding": "DEVICE_USAGE",
      "dataset": "device_usage"
    },
    {
      "binding": "BANDWIDTH_USAGE",
      "dataset": "bandwidth_usage"
    },
    {
      "binding": "VERSION_USAGE",
      "dataset": "version_usage"
    },
    {
      "binding": "APP_LOG",
      "dataset": "app_log"
    },
    {
      "binding": "APP_LOG_EXTERNAL",
      "dataset": "app_log_external"
    }
  ],
  "d1_databases": [
    {
      "binding": "DB_DEVICES",
      "database_name": "capgo_prod_devices",
      "database_id": "2510b55c-d6a2-4595-a2a5-d9aca82a3753"
    },
    {
      "binding": "DB_STOREAPPS",
      "database_name": "capgo_prod_storeapps",
      "database_id": "81236a0c-db6e-454d-87da-944fa9bc100c"
    },
    {
      "binding": "DB_REPLICATE",
      "database_name": "capgo_prod_replicate",
      "database_id": "7f267d86-f4f7-49f5-9622-e44373aa00dc"
    }
  ],
  "env": {
    "preprod": {
      "name": "capgo_plugin-preprod",
      "vars": {
        "is_v2": "1"
      },
      "observability": {
        "enabled": true
      },
      "routes": [
        {
          "pattern": "plugin_v2.preprod.capgo.app",
          "custom_domain": true
        },
        {
          "pattern": "plugin_v2.preprod.usecapgo.com",
          "custom_domain": true
        }
      ],
      "d1_databases": [
        {
          "binding": "DB_DEVICES",
          "database_name": "capgo_prod_devices",
          "database_id": "2510b55c-d6a2-4595-a2a5-d9aca82a3753"
        },
        {
          "binding": "DB_STOREAPPS",
          "database_name": "capgo_prod_storeapps",
          "database_id": "81236a0c-db6e-454d-87da-944fa9bc100c"
        },
        {
          "binding": "DB_REPLICATE",
          "database_name": "capgo_prod_replicate",
          "database_id": "7f267d86-f4f7-49f5-9622-e44373aa00dc"
        }
      ],
      "analytics_engine_datasets": [
        {
          "binding": "DEVICE_USAGE",
          "dataset": "device_usage"
        },
        {
          "binding": "BANDWIDTH_USAGE",
          "dataset": "bandwidth_usage"
        },
        {
          "binding": "VERSION_USAGE",
          "dataset": "version_usage"
        },
        {
          "binding": "APP_LOG",
          "dataset": "app_log"
        },
        {
          "binding": "APP_LOG_EXTERNAL",
          "dataset": "app_log_external"
        }
      ]
    },
    "dev": {
      "name": "capgo_plugin-dev",
      "observability": {
        "enabled": true
      },
      "vars": {
        "is_v2": "1"
      },
      "routes": [
        {
          "pattern": "plugin_v2.dev.capgo.app",
          "custom_domain": true
        },
        {
          "pattern": "plugin_v2.dev.usecapgo.com",
          "custom_domain": true
        }
      ],
      "d1_databases": [
        {
          "binding": "DB_DEVICES",
          "database_name": "capgo_alpha_devices",
          "database_id": "2510b55c-d6a2-4595-a2a5-d9aca82a3753"
        },
        {
          "binding": "DB_STOREAPPS",
          "database_name": "capgo_prod_storeapps",
          "database_id": "81236a0c-db6e-454d-87da-944fa9bc100c"
        },
        {
          "binding": "DB_REPLICATE",
          "database_name": "capgo_alpha_replicate",
          "database_id": "7f267d86-f4f7-49f5-9622-e44373aa00dc"
        }
      ],
      "analytics_engine_datasets": [
        {
          "binding": "DEVICE_USAGE",
          "dataset": "device_usage_alpha"
        },
        {
          "binding": "BANDWIDTH_USAGE",
          "dataset": "bandwidth_usage_alpha"
        },
        {
          "binding": "VERSION_USAGE",
          "dataset": "version_usage_alpha"
        },
        {
          "binding": "APP_LOG",
          "dataset": "app_log_alpha"
        },
        {
          "binding": "APP_LOG_EXTERNAL",
          "dataset": "app_log_external_alpha"
        }
      ]
    },
    "local": {
      "name": "capgo_plugin-local"
    }
  }
}
