{
  "mcpServers": {
    "context7": {
      "command": "bunx",
      "args": ["-y", "@upstash/context7-mcp"]
    },
    "browser": {
      "command": "bunx",
      "args": [
        "@agentdeskai/browser-tools-mcp"
      ],
      "env": {}
    },
    "claude-code": {
      "command": "claude",
      "args": ["mcp", "serve"],
      "env": {}
    },
    "supabase-local": {
      "command": "bunx",
      "args": [
        "@modelcontextprotocol/server-postgres",
        "postgresql://postgres:postgres@127.0.0.1:54322/postgres"
      ]
    }
  }
}
